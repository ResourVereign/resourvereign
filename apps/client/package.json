    "@slangy/client": "^2.0.1",
    "@slangy/react": "^2.2.0",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "formik": "^2.4.4",
    "i18next": "^23.6.0",
    "i18next-browser-languagedetector": "^7.1.0",
    "install": "^0.13.0",
    "jwt-decode": "^4.0.0",
